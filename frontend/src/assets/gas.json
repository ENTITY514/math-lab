emp = value\n          this.speed = Math.sqrt(3*8.31*this.temp/(this.mass*100))\n        })\n        this.core.input_module.register(\"number\")\n        this.core.input_module.inputs[1].onChange((value)=>{\n          if(value>0){\n            this.mass = value\n            this.speed = Math.sqrt(3*8.31*this.temp/(this.mass*100))\n          }\n          else{\n             this.speed = 0\n          }\n        })\n        \n        this.right = this.obj_module.findByTag(\"right\")[0]\n        this.left = this.obj_module.findByTag(\"left\")[0]\n        this.bottom = this.obj_module.findByTag(\"bottom\")[0]\n        this.top = this.obj_module.findByTag(\"top\")[0]\n        this.r_point = this.obj_module.findByTag(\"point_red\")[0]\n        this.r_point.transform.setSize(this.size,this.size)\n        for(let i = 0; i<this.count;i++){\n          let obj = this.obj_module.createObject(\"sprite\")\n          obj.components[1].setTexture(this.r_point.texture_file)\n          obj.components[1].changeTint(this.r_point.components[1].getTint())\n          obj.transform.setSize(this.size,this.size)\n          obj.transform.setPosition(Math.random()*1000-500,Math.random()*1000-500)\n          obj.setTag(\"point_red\")\n        }\n        this.r_points =  this.obj_module.findByTag(\"point_red\")\n      }\n  \n      onUpdate() {\n        for (let i = 0; i < this.r_points.length; i++) {\n          let pos = this.r_points[i].transform.getPosition()\n          if(pos.x>this.right.transform.position.x | pos.x<this.left.transform.position.x ){\n            this.dir[i].x = -this.dir[i].x\n          }\n          if(pos.y>this.bottom.transform.position.y | pos.y<this.top.transform.position.y){\n            this.dir[i].y = -this.dir[i].y\n          }\n          for (let j = 0; j < this.r_points.length; j++) {\n            if(i!==j){\n              let dif_x = this.r_points[i].transform.position.x - this.r_points[j].transform.position.x\n              let dif_y = this.r_points[i].transform.position.y - this.r_points[j].transform.position.y\n              let dist = Math.sqrt(dif_x*dif_x+dif_y*dif_y)\n              if(dist<=this.size){\n                let dir = {x:this.dir[i].x, y:this.dir[i].y }\n                this.dir[i].x = -this.dir[j].x\n                this.dir[i].y = -this.dir[j].y\n                this.dir[j].x = - dir.x\n                this.dir[j].y = - dir.y\n              }\n            }\n          }\n          this.r_points[i].transform.setPosition(pos.x+this.dir[i].x*this.speed,pos.y+this.dir[i].y*this.speed)\n        }\n      }\n  }","is_file":true},{"id":"1DAJaaSfPlPSivIK22_EC","name":"script_fileK3aRpy8FnTRS18HFFUyvS","type":"mljs","url":"/eogu49ZJIVW-QcXfpiO4Q.mljs","metadata":null,"data":"\n    return class ScriptObject {\n      object;\n      constructor(object) {\n          this.object = object\n      }\n  \n      onStart() {\n        this.core = globalThis.core\n        this.obj_module = this.core.object_module\n        \n      }\n  \n      onUpdate() {\n      }\n  }\n    ","is_file":true}],"is_file":false}}